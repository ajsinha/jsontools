{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SimpleUser",
  "type": "object",
  "required": ["userId", "fullName", "status"],
  "properties": {
    "userId": { "type": "string" },
    "status": { "type": "string", "enum": ["ACTIVE", "INACTIVE"] },
    "fullName": { "type": "string" },
    "contactEmail": { "type": "string" },
    "age": { "type": "integer" },
    "address": {
      "type": "object",
      "properties": {
        "line1": { "type": "string" },
        "city": { "type": "string" },
        "postalCode": { "type": "string" }
      }
    },
    "interests": {
      "type": "array",
      "items": { "type": "string" }
    },
    "version": { "type": "string" },
    "transformedAt": { "type": "string" },
    "transformId": { "type": "string" }
  }
}
